<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView_144"
        android:layout_width="110dp"
        android:layout_height="89dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="26dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_144"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logo"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="143dp" />

    <TextView
        android:id="@+id/textView2_144"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bank"
        android:textSize="48sp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/textView3_144"
        android:layout_width="248dp"
        android:layout_height="93dp"
        android:text="Please log in to your bank account and enter TAN me followed by it"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="264dp" />

    <Button
        android:id="@+id/button2_144"
        android:layout_width="263dp"
        android:layout_height="64dp"
        android:text="Cancel"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="576dp" />

    <Button
        android:id="@+id/button_144"
        android:layout_width="263dp"
        android:layout_height="65dp"
        android:text="Login to bank account"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="475dp" />

    <ProgressBar
        android:id="@+id/progressBar_144"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="368dp" />

</androidx.constraintlayout.widget.ConstraintLayout>